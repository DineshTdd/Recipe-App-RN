{
  "expo": {
    "name": "Recipe-App",
    "slug": "recipe-app",
    "owner": "dineshtd",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "description": "A simple Recipe App with filters and favorites.",
    "githubUrl": "https://github.com/DineshTdd/Recipe-App-RN",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.td.recipeapp"
    }
  }
}